@model SAGM.Data.Entities.WorkOrderDetailProcess

@{
     Layout = null;
}



<style>
    .g1 {
        background-color: #F9F9F9;
        margin: 10px 10px 10px 10px;
        border: 2px solid orange;
        border-radius: 12px;
        box-shadow: 5px 5px 5px #555;
        background-color: whitesmoke;
        padding: 1px 2px 2px 1px;
    }
</style>

<div tabindex="-1" data-bs-backdrop="static" aria-labelledby="exampleModalLabel" aria-hidden="true">


    <div class="modal-body">
        <p>¿Estás seguro que deseas borrar el proceso?</p>
    </div>
    <div class="modal-footer">
        <div class="col text-center">
            <form asp-action="DeleteProcess" onsubmit="return jQueryAjaxPost(this);">
                <input type="hidden" asp-for="WorkOrderDetailProcessId" />
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal" arial-label="cerrar"><i class="fa fa-ban"></i> No</button>
                <button type="submit" value="Si" class="btn btn-danger"><i class="fa fa-check-circle"></i> Sí</button>
            </form>
        </div>

    </div>


</div>
